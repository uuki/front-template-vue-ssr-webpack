  "scripts": {
    "build": "yarn run build:client && yarn run build:server",
    "build:client": "node dev/build.js",
    "build:server": "webpack --config ./dev/webpack.server.config.js",
    "watch": "concurrently --kill-others \"yarn run watch:dev\" \"yarn run watch:serve\" \"yarn run serve:nodemon\"",
    "watch:dev": "webpack --config ./dev/webpack.dev.config.js --watch",
    "watch:dev-server": "webpack-dev-server --inline --progress --config ./dev/webpack.dev.config.js",
    "watch:serve": "webpack --config ./dev/webpack.server.config.js --watch",
    "serve:nodemon": "nodemon ./dev/server.js",
    "serve:prod": "node dev/server.js",
    "imagemin": "node dev/imagemin.js"
  },
